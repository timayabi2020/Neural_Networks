   "source": [
    "def which_value(length, width):\n",
    "    z = length * w1 + width * w2 + b\n",
    "    pred = sigmoid(z)\n",
    "    if pred < .5:\n",
    "        speak.Speak(\"The point is blue in color\")\n",
    "    else:\n",
    "        speak.Speak(\"The point is red in color\")\n",
    "            \n",
    "        \n",
    "    "
   ]
  },
